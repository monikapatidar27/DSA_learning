#include<bits/stdc++.h>
using namespace std;
#define loop(i,l,n) for(int i=0;i<n;i++)
#define loop1(i,l,n) for(int i=1;i<=n;i++)
#define endl "\n"
typedef long long int ll;
#define tle ios_base::sync_with_stdio(false); cin.tie(NULL);
#define sp " "
int main(){
   int n;
cin>>n;
int  past=0;
int min=1200;
int max=0;
int count=0;
for(int i=0;i<n;i++){
	 int x;
	 cin>>x;
	 if(!i){
	 	min=x;
	 	max=min;
	 
	 }
	 else {
	 	
	 	if(x>max){
	 		max=x;
	 		++count;
	 }
	 else if(x<min){
	 	++count;
	 	min=x;
	 	
	 }
	  
 
}}
cout<<count<<endl;


    return 0;
}